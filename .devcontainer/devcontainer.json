{
    "customizations": {
        "vscode": {
            "extensions": [
                "/workspace/.devcontainer/devcontainer-utils.vsix",
                "esbenp.prettier-vscode",
                "ms-python.black-formatter",
                "ms-python.isort",
                "ms-python.python",
                "ms-python.vscode-pylance",
                "njpwerner.autodocstring",
                "rohit-gohri.format-code-action",
                "usernamehw.errorlens"
            ],
            "settings": {
                "[dockercompose]": {
                    "editor.codeActionsOnSave": [
                        "source.sortImports",
                        "source.formatDocument"
                    ],
                    "editor.defaultFormatter": "esbenp.prettier-vscode"
                },
                "[javascript]": {
                    "editor.codeActionsOnSave": [
                        "source.sortImports",
                        "source.formatDocument"
                    ],
                    "editor.defaultFormatter": "esbenp.prettier-vscode"
                },
                "[javascriptreact]": {
                    "editor.codeActionsOnSave": [
                        "source.sortImports",
                        "source.formatDocument"
                    ],
                    "editor.defaultFormatter": "esbenp.prettier-vscode"
                },
                "[json]": {
                    "editor.codeActionsOnSave": [
                        "source.sortImports",
                        "source.formatDocument"
                    ],
                    "editor.defaultFormatter": "esbenp.prettier-vscode"
                },
                "[jsonc]": {
                    "editor.codeActionsOnSave": [
                        "source.sortImports",
                        "source.formatDocument"
                    ],
                    "editor.defaultFormatter": "esbenp.prettier-vscode"
                },
                "[markdown]": {
                    "editor.codeActionsOnSave": [
                        "source.sortImports",
                        "source.formatDocument"
                    ],
                    "editor.defaultFormatter": "esbenp.prettier-vscode"
                },
                "[python]": {
                    "editor.codeActionsOnSave": [
                        "source.organizeImports",
                        "source.formatDocument"
                    ],
                    "editor.defaultFormatter": "ms-python.black-formatter"
                },
                "[typescript]": {
                    "editor.codeActionsOnSave": [
                        "source.sortImports",
                        "source.formatDocument"
                    ],
                    "editor.defaultFormatter": "esbenp.prettier-vscode"
                },
                "[typescriptreact]": {
                    "editor.codeActionsOnSave": [
                        "source.sortImports",
                        "source.formatDocument"
                    ],
                    "editor.defaultFormatter": "esbenp.prettier-vscode"
                },
                "[yaml]": {
                    "editor.codeActionsOnSave": [
                        "source.sortImports",
                        "source.formatDocument"
                    ],
                    "editor.defaultFormatter": "esbenp.prettier-vscode"
                },
                "isort.args": [
                    "--profile",
                    "black"
                ],
                "python.defaultInterpreterPath": "/devcontainer-utils/asdf/shims/python"
            }
        }
    },
    "dockerComposeFile": [
        "docker-compose.yaml"
    ],
    "name": "devcontainer",
    "postCreateCommand": "/workspace/.devcontainer/post-create.sh",
    "service": "devcontainer",
    "workspaceFolder": "/workspace"
}